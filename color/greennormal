#!/bin/sh

# <html><h2>greennormal</h2>
# <h3>Examples</h3>
# <dl>
# <dt>Show arguments with space in GreenNormal:</dt>
# <dd>
# <pre><code class="language-bash">greennormal Show Text
# </code></pre>
# </dd>
# </dl>
# </html>
[ $# -eq 0 ] || printf -- '%b' "\033[0;32m"; printf '%b' "$*"; printf -- '%b' "\033[0m"
